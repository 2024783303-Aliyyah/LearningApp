<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.microlearningquizapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="36" />

    <permission
        android:name="com.example.microlearningquizapp.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.microlearningquizapp.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.MicroLearningQuizApp" >
        <activity
            android:name="com.example.microlearningquizapp.Report"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.AdminProfile"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.ScienceQuizzes"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.EnglishQuizzes"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.AddQuiz"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.MathQuizzes"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.quizzesmodules"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.ViewLesson"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.EditLesson"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.learningmodules"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.lessonActivity"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.StudentDashboardActivity"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.UserProfileActivity"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.MathLessons"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.ScienceLessons"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.EnglishLessons"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.adminDashboard"
            android:exported="false" />
        <activity
            android:name="com.example.microlearningquizapp.SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.microlearningquizapp.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>